<Page x:Class="BuilPlanner.SkillSearchPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:BuilPlanner"
      mc:Ignorable="d" 
      d:DesignHeight="600" d:DesignWidth="800"
      Title="SkillSearchPage">

    <Canvas x:Name="Can_SkillSearch">
        <Grid Margin="10,10,-10,-10" Width="{Binding ActualWidth, ElementName=Can_SkillSearch}" Height="{Binding ActualHeight, ElementName=Can_SkillSearch}" >
            <Grid.RowDefinitions>
                <RowDefinition Height="100"></RowDefinition>
                <RowDefinition Height="1*"></RowDefinition>
            </Grid.RowDefinitions>
            <StackPanel x:Name="Sp_Skill_Search" Grid.Row="1"></StackPanel>
        </Grid>
    </Canvas>

</Page>
